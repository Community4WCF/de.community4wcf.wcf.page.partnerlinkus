<?xml version="1.0" encoding="UTF-8"?>
<!-- $Id: groupoptions.xml 1264 2010-04-13 15:17:31Z TobiasH87 $ -->
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com/XSD/group-options.xsd">
	<import>
		<!-- A new tab and subtab is added to group rights. -->
		<categories>
			<category name="user.managepages">
				<parent>user</parent>
			</category>
			<category name="admin.content.partner">
				<parent>admin.content</parent>
				<options>module_partner</options>
				<showorder>45</showorder>
			</category>
			<category name="admin.content.linkus">
				<parent>admin.content</parent>
				<options>module_linkus</options>
				<showorder>46</showorder>
			</category>
		</categories>
		<!-- A new rights is added to group rights. -->
		<options>
			<option name="user.managepages.canViewPartner">
				<categoryname>user.managepages</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<showorder>15</showorder>
				<options>module_partner</options>
			</option>
			<option name="user.managepages.canViewLinkUs">
				<categoryname>user.managepages</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<showorder>16</showorder>
				<options>module_linkus</options>
			</option>
			<option name="admin.content.partner.partnershow">
				<categoryname>admin.content.partner</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<showorder>1</showorder>
				<enableoptions><![CDATA[admin.content.partner.partneradd,admin.content.partner.partneredit,admin.content.partner.partnerdelete]]></enableoptions>
			</option>
			<option name="admin.content.partner.partneradd">
				<categoryname>admin.content.partner</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<showorder>2</showorder>
			</option>
			<option name="admin.content.partner.partneredit">
				<categoryname>admin.content.partner</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<showorder>3</showorder>
			</option>
			<option name="admin.content.partner.partnerdelete">
				<categoryname>admin.content.partner</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<showorder>4</showorder>
			</option>
			<option name="admin.content.partner.partneroptions">
				<categoryname>admin.content.partner</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<showorder>5</showorder>
			</option>
			<option name="admin.content.linkus.linkusshow">
				<categoryname>admin.content.linkus</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<showorder>1</showorder>
				<enableoptions><![CDATA[admin.content.linkus.linkusadd,admin.content.linkus.linkusedit,admin.content.linkus.linkusdelete]]></enableoptions>
			</option>
			<option name="admin.content.linkus.linkusadd">
				<categoryname>admin.content.linkus</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<showorder>2</showorder>
			</option>
			<option name="admin.content.linkus.linkusedit">
				<categoryname>admin.content.linkus</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<showorder>3</showorder>
			</option>
			<option name="admin.content.linkus.linkusdelete">
				<categoryname>admin.content.linkus</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<showorder>4</showorder>
			</option>
			<option name="admin.content.linkus.linkusoptions">
				<categoryname>admin.content.linkus</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<showorder>5</showorder>
			</option>
		</options>
	</import>
</data>