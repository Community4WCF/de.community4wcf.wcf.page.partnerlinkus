<?xml version="1.0" encoding="UTF-8"?>
<!-- $Id: options.xml 1269 2010-04-13 16:58:05Z TobiasH87 $ -->
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com/XSD/options.xsd">
	<import>
		<categories>
			<category name="partner.partnerapply">
				<parent>partner</parent>
				<permissions>admin.content.partner.partnerapply</permissions>
			</category>
			<category name="partner.partnerapply.support">
				<parent>partner.partnerapply</parent>
			</category>
		</categories>
		<options>
			<!-- page options -->
			<option name="partnerapply_title">
				<categoryname>partner.partnerapply</categoryname>
				<optiontype>text</optiontype>
				<defaultvalue>Voraussetzungen</defaultvalue>
				<showorder>1</showorder>
			</option>
			<option name="partnerapply_text">
				<categoryname>partner.partnerapply</categoryname>
				<optiontype>textarea</optiontype>
				<defaultvalue><![CDATA[[list][*]Keine Illegalen Inhalte[*]Aktives Forum/Webseite[*]Ähnliches Themengebiet[*]Gegenseitiger Banneraustausch und Einbindung[/list]Voraussetzungen erfüllt/akzeptiert ? - jetzt beim Team bewerben.]]></defaultvalue>
				<showorder>2</showorder>
			</option>
			<option name="partnerapply_enable_smilies">
				<categoryname>partner.partnerapply</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
				<showorder>3</showorder>
			</option>
			<option name="partnerapply_enable_html">
				<categoryname>partner.partnerapply</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<showorder>4</showorder>
			</option>
			<option name="partnerapply_enable_bbcodes">
				<categoryname>partner.partnerapply</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
				<showorder>5</showorder>
			</option>
			<!-- support options -->
			<option name="partnerapply_enable_support">
				<categoryname>partner.partnerapply.support</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<showorder>1</showorder>
				<enableoptions><![CDATA[partnerapply_support_type,partnerapply_support_link]]></enableoptions>
			</option>
			<option name="partnerapply_support_type">
				<categoryname>partner.partnerapply.support</categoryname>
				<optiontype>text</optiontype>
				<defaultvalue>via Email</defaultvalue>
				<showorder>2</showorder>
			</option>
			<option name="partnerapply_support_link">
				<categoryname>partner.partnerapply.support</categoryname>
				<optiontype>text</optiontype>
				<defaultvalue><![CDATA[index.php?form=Mail&amp;userID=1]]></defaultvalue>
				<showorder>3</showorder>
			</option>
			<!-- /support options -->
			<!-- /page options -->
		</options>
	</import>
</data>